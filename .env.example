# Anthropic API Key (server-side only - do not prefix with VITE_)
ANTHROPIC_API_KEY=your-anthropic-key

# Supabase Public (accessible in browser)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-publishable-key

# Supabase Public (for SvelteKit's $env/static/public)
PUBLIC_SUPABASE_URL=https://your-project.supabase.co

# Supabase Secret Key (server-side only - for admin operations)
# Get this from Supabase Dashboard > API keys > secret key
SUPABASE_SECRET_KEY=your-secret-key
