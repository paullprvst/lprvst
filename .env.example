# Supabase Public (accessible in browser via $env/static/public)
PUBLIC_SUPABASE_URL=https://your-project.supabase.co
PUBLIC_SUPABASE_ANON_KEY=your-publishable-key

# Supabase Secret Key (server-side only)
# Get this from Supabase Dashboard > API keys > secret key
SUPABASE_SECRET_KEY=your-secret-key

# Optional: YouTube Data API key for exercise demo videos
# Create one at https://console.cloud.google.com/apis/credentials
YOUTUBE_DATA_API_KEY=your-youtube-data-api-key

# Versioning rollout flags (client-visible)
# Set to false to fall back to legacy JSON reads/writes.
PUBLIC_PROGRAM_VERSIONING_READS=true
PUBLIC_PROGRAM_VERSIONING_WRITES=true

# Agent rollout flag (client/server-visible)
# Set to false to disable tool-calling orchestration.
PUBLIC_AGENT_TOOL_CALLING=true
